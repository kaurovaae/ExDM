{
  "name": "ExDM",
  "description": "Expiration date monitoring",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npx concurrently -r --kill-others npm:front npm:back",
    "front": "webpack serve --mode=development --config ./build/webpack/client.config.js",
    "back": "nodemon ./src/server.js",
    "build": "webpack --mode=development --config ./build/webpack/client.config.js",
    "build-prod": "webpack --mode=production --config ./build/webpack/client.config.js",
    "clean": "rd /s /q dist"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
